#Написать метод bananas, который принимает на вход строку и
#возвращает количество слов «banana» в строке.
#
#(Используйте - для обозначения зачеркнутой буквы)
#
#Input: bbananana
#
#Output:
#
#b-anana--
#b-anan--a
#b-ana--na
#b-an--ana
#b-a--nana
#b---anana
#-banana--
#-banan--a
#-bana--na
#-ban--ana
#-ba--nana
#-b--anana


from itertools import combinations


def bananas(s) -> set:
    result = set()
    for i in combinations(range(1, len(s) - 1), 3):
        m = list(s)
        for j in i: m[j] = '-'
        if [k for k in m if k != '-'] == list('banana'): result.add(''.join(m))
    return result

print(bananas('bbananana'))
